                    {{#each project}}
                    <a href="{{ID}}"
                        class="portfolio-piece d-flex align-items-center shadow justify-content-center col-md-6 {{field_name}}"
                        style="background-image: url(../{{thumbnail}});">

                        <h4 class=" display-4">{{name}}</h4>
                        <div class="overlay"></div>
                    </a>


                    {{/each}}
                    {{!-- </div> <button class="mt-5">See more</button> --}}
                </div>
            </div>
        </div>
    </section>


